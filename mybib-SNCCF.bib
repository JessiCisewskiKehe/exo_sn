	Doi = {10.1126/science.1253253},
	Eprint = {1407.1049},
	Journal = {Science},
	Keywords = {Astrophysics - Earth and Planetary Astrophysics},
	Month = jul,
	Pages = {440-444},
	Primaryclass = {astro-ph.EP},
	Title = {{Stellar activity masquerading as planets in the habitable zone of the M dwarf Gliese 581}},
	Volume = 345,
